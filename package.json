{
  "name": "adonisjs-web-stater-kit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "export": "vite build && npm run download:sponsors && node --loader=ts-node/esm bin/build.ts",
    "build": "npm run export",
    "postexport": "copyfiles -u 1 public/* public/**/* dist",
    "download:sponsors": "node --loader=ts-node/esm bin/download_sponsors.ts",
    "start": "node bin/test.js",
    "serve": "node --loader=ts-node/esm bin/serve.ts",
    "dev": "concurrently \"vite\" \"npm run serve\"",
    "test": "node ace test"
  },
  "imports": {
    "#src/*": "./src/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^6.1.3-22",
    "@adonisjs/tsconfig": "^1.1.8",
    "@adonisjs/vite": "^0.0.1-9",
    "@alpinejs/persist": "^3.13.1",
    "@dimerapp/content": "^4.1.0",
    "@dimerapp/docs-theme": "^3.0.2",
    "@dimerapp/edge": "^4.0.0",
    "@docsearch/css": "^3.5.2",
    "@docsearch/js": "^3.5.2",
    "@swc/core": "1.3.82",
    "@types/node": "^20.8.2",
    "alpinejs": "^3.13.1",
    "collect.js": "^4.36.1",
    "concurrently": "^8.2.1",
    "copyfiles": "^2.4.1",
    "edge-uikit": "^1.0.0-1",
    "medium-zoom": "^1.0.8",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "undici": "^5.25.4",
    "unpoly": "^3.3.0",
    "vite": "^4.4.11"
  },
  "dependencies": {
    "@adonisjs/core": "^6.1.5-26",
    "@adonisjs/static": "^1.0.3-1",
    "@radix-ui/colors": "^3.0.0",
    "edge.js": "^6.0.0-10"
  }
}
